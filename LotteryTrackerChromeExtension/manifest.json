{
    "manifest_version": 2,

    "name": "Lottery Tracker",
    "description": "Descripcion",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "background.html"
    },
    
    "permissions": [
        "tabs",
        "downloads"        
    ],

    "content_scripts": [ {
        "js": [ "jquery-3.2.1.js", "contentscript.js" ],
        "matches": [ "http://*/*", "https://*/*"]
        }
    ]
}